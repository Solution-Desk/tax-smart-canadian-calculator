# Base URL for the application (empty for root, or '/your-repo-name' for GitHub Pages)
VITE_BASE_URL=/

# Clerk Authentication
VITE_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here

# Stripe (for billing)
